      <column type="gchararray"/>
    </columns>
    <data>
      <row>
        <col id="0" translatable="yes">Insert at cursor</col>
      </row>
      <row>
        <col id="0" translatable="yes">Replace selection or document</col>
      </row>
      <row>
        <col id="0" translatable="yes">New Document</col>
      </row>
    </data>
  </object>
  <object class="GtkVBox" id="root">
